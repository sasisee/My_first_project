pipeline {
    agent none
    stages {
        stage('build') {
            steps {
                echo 'ssk building the application'
            }
        }
        stage('test') {
            steps {
                echo 'ssk testing the application'
            }
        }
        stage('deploy') {
            steps {
                echo 'ssk deploying the application'
            }
        }
    }
}
